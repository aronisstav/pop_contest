<.header>
  GASP! Leaderboard
  <:actions>
    <.link href={~p"/vote"} >
      <.button class="bg-blue-500 w-full">Go to vote!</.button>
    </.link>
  </:actions>
</.header>

<.table
  id="songs"
  rows={@songs}
>
  <:col :let={song}><%= song.title %></:col>
  <:col :let={song}><%= song.tally %></:col>
</.table>
